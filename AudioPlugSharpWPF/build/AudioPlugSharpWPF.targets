<?xml version="1.0" encoding="utf-8"?>
<Project  xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup>
    <Content Include="$(MSBuildThisFileDirectory)/../lib/net6.0-windows7.0/AudioPlugSharpWPF.dll">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <Visible>false</Visible>
    </Content>
  </ItemGroup>

  <Target Name="AudioPlugSharpWPF" AfterTargets="AudioPlugSharp">
    <Message Text="Copying AudioPlugSharpWPF files to output directory"></Message>
    <Copy SourceFiles="$(MSBuildThisFileDirectory)/../contentFiles/wpf.runtimeconfig.json" DestinationFiles="$(TargetDir)$(TargetName)Bridge.runtimeconfig.json"></Copy>
  </Target>
</Project>